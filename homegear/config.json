{
    "name": "Homegear",
    "version": "1.0.9",
    "slug": "homegear",
    "description": "Homegear hassio add-on",
    "uart": true,
    "url": "https://github.com/rcreate/hassio-homegear",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "apparmor": false,
    "ports": {
      "2001/tcp": 2001,
      "80/tcp": 8124
    },
    "ports_description": {
      "2001/tcp": "HomeGear UI",
      "8124/tcp": "HomeGear Admin"
    },
    "map": [
      "config:rw",
      "ssl"
    ],
    "devices": [
      "/dev/spidev0.0",
      "/dev/spidev0.1"
    ],
    "gpio": true,
    "schema": {},
    "options": {},
    "ingress": true,
    "ingress_port": 8124,
    "panel_icon": "mdi:beach"
}
